{
  "name": "vodafone-station-cli",
  "description": "Access your Vodafone Station from the comfort of the command line.",
  "version": "1.2.8",
  "author": "Dobroslav Totev",
  "bin": {
    "vodafone-station-cli": "./bin/run"
  },
  "homepage": "https://github.com/totev/vodafone-station-cli",
  "license": "MIT",
  "main": "lib/index.js",
  "bugs": "https://github.com/totev/vodafone-station-cli/issues",
  "repository": "totev/vodafone-station-cli",
  "files": [
    "/bin",
    "/lib",
    "!/lib/**/__fixtures__/",
    "!/lib/**/*.test.*",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "keywords": [
    "vodafone",
    "station",
    "cli",
    "docsis",
    "cable-modem"
  ],
  "dependencies": {
    "@oclif/config": "^1",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^1.7.7",
    "axios-cookiejar-support": "^5.0.3",
    "brotli": "^1.3.3",
    "dotenv": "^16.4.5",
    "sjcl": "1.0.8",
    "tough-cookie": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^18.0.0",
    "@types/sjcl": "^1.0.34",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "eslint": "^9.11.1",
    "globby": "^14",
    "jest": "^29.7.0",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  },
  "oclif": {
    "bin": "vodafone-station-cli",
    "dirname": "vodafone-station-cli",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "host-exposure": {
        "description": "Manage IPv6 host exposure settings."
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf lib && tsc -b",
    "build:standalone": "oclif pack tarballs",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "types": "lib/index.d.ts"
}