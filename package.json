{
  "name": "vodafone-station-cli",
  "description": "Access your Vodafone Station from the comfort of the command line.",
  "version": "1.5.3",
  "author": "Dobroslav Totev",
  "bin": {
    "vodafone-station-cli": "./bin/run"
  },
  "homepage": "https://github.com/totev/vodafone-station-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "bugs": "https://github.com/totev/vodafone-station-cli/issues",
  "repository": "totev/vodafone-station-cli",
  "files": [
    "/bin",
    "/dist",
    "!/dist/**/__fixtures__/",
    "!/dist/**/*.test.*",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "keywords": [
    "vodafone",
    "station",
    "cli",
    "docsis",
    "cable-modem"
  ],
  "dependencies": {
    "@oclif/core": "^4.4.0",
    "@oclif/plugin-help": "^6.2.29",
    "@oclif/plugin-plugins": "^5.4.40",
    "axios": "^1.9.0",
    "axios-cookiejar-support": "^6.0.2",
    "brotli": "^1.3.3",
    "dotenv": "^16.5.0",
    "http-cookie-agent": "^7.0.1",
    "sjcl": "1.0.8",
    "tough-cookie": "^5.1.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.1",
    "@types/sjcl": "^1.0.34",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "eslint": "^9.31.0",
    "eslint-config-oclif": "^6.0.68",
    "eslint-config-prettier": "^10.1.5",
    "globby": "^14.1.0",
    "jest": "^30.0.0",
    "oclif": "^4.18.1",
    "shx": "^0.4.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "oclif": {
    "bin": "vodafone-station-cli",
    "dirname": "vodafone-station-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "host-exposure": {
        "description": "Manage IPv6 host exposure settings."
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:standalone": "oclif pack tarballs",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "types": "dist/index.d.ts"
}