{
  "name": "vodafone-station-cli",
  "description": "Access your Vodafone Station from the comfort of the command line.",
  "version": "1.2.8",
  "author": "Dobroslav Totev",
  "bin": {
    "vodafone-station-cli": "./bin/run"
  },
  "homepage": "https://github.com/totev/vodafone-station-cli",
  "license": "MIT",
  "main": "lib/index.js",
  "bugs": "https://github.com/totev/vodafone-station-cli/issues",
  "repository": "totev/vodafone-station-cli",
  "files": [
    "/bin",
    "/lib",
    "!/lib/**/__fixtures__/",
    "!/lib/**/*.test.*",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "keywords": [
    "vodafone",
    "station",
    "cli",
    "docsis",
    "cable-modem"
  ],
  "dependencies": {
    "@oclif/config": "^1",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.27.2",
    "axios-cookiejar-support": "^4.0.2",
    "brotli": "^1.3.3",
    "dotenv": "^16.0.1",
    "sjcl": "^1.0.8",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/sjcl": "^1.0.30",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "globby": "^11",
    "jest": "^28.1.1",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "oclif": {
    "bin": "vodafone-station-cli",
    "dirname": "vodafone-station-cli",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf lib && tsc -b",
    "build:standalone": "oclif pack tarballs",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "types": "lib/index.d.ts"
}
