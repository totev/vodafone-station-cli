{
  "name": "vodafone-station-cli",
  "description": "Access your Arris TG3442DE (aka Vodafone Station) from the comfort of the command line.",
  "version": "1.0.0",
  "author": "Dobroslav Totev",
  "bin": {
    "vodafone-station-cli": "./bin/run"
  },
  "bugs": "https://github.com/totev/vodafone-station-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.1",
    "node-fetch": "^2.6.1",
    "sjcl": "^1.0.8",
    "tough-cookie": "^4.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/jest": "^26.0.20",
    "@types/node": "^10",
    "@types/sjcl": "^1.0.29",
    "@types/tough-cookie": "^4.0.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^8",
    "tslib": "^1",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/totev/vodafone-station-cli",
  "keywords": [],
  "license": "MIT",
  "main": "src/index.ts",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vodafone-station-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "totev/vodafone-station-cli",
  "scripts": {
    "build:standalone":"oclif-dev pack",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "lint":"eslint . --ext .ts --config .eslintrc"
  },
  "types": "lib/index.d.ts"
}
